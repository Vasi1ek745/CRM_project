
<%= turbo_frame_tag "edit", data: {turbo_action: 'advance'}  do %>

    <div class="my-4 flex justify-end">
        <% if user_signed_in? %>
            <%= link_to'New client' , new_client_path, class: "py-2.5 px-5 mr-2 mb-2 text-sm font-medium text-gray-900 focus:outline-none bg-white rounded-lg border border-gray-200 hover:bg-gray-100 hover:text-blue-700 focus:z-10 focus:ring-4 focus:ring-gray-200" %>
        <% else %>
            <%= link_to'New client' , new_user_session_path, class: "py-2.5 px-5 mr-2 mb-2 text-sm font-medium text-gray-900 focus:outline-none bg-white rounded-lg border border-gray-200 hover:bg-gray-100 hover:text-blue-700 focus:z-10 focus:ring-4 focus:ring-gray-200", data: {turbo: false} %>
        <% end %>

    </div>

<% end %>

<%= render 'clients/clients_table' %>

 



