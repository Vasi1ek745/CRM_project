<!DOCTYPE html>
<html>
  <head>
    <title>CRMProject</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "tailwind", "inter-font", "data-turbo-track": "reload" %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_importmap_tags %>
    

  </head>

  <body>
    <main >


      <%= render 'main_page/nav' %>
      <%= render 'main_page/aside' %>

      <div class="py-4 pr-3 sm:ml-64">
        <div class="p-3 border-2 border-gray-200 border-dashed rounded-lg  mt-14 h-full">

          <div id = "flash">
            <%= render "shared/flash" %>
          </div>
            <%= yield %>

        </div>
      </div>

    </main>
  </body>
</html>
